say server_socket: u64 = @socket_create
@socket_bind server_socket, 8888
@socket_listen server_socket, 5

while 1 > 0 {
    say client_socket: u64 = @socket_accept server_socket
    say response: [101]u8 = "HTTP/1.1 200\ncontent-type: text/html\ncontent-length: 48\n\n<title>Garbage</title><body><b>we ball</b></body>"

    @socket_write client_socket, response, 101
    @socket_close client_socket 
}

